<Window x:Class="AFRIDAY.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AFRIDAY"
        mc:Ignorable="d"
         Title="Enigma Machine" Height="778" Width="1319" Background="LightGray" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp">
    <Grid>
        <StackPanel Margin="0,0,0,664">
            <Menu Background="#F1EFEF">
                <MenuItem Header="File">
                    <MenuItem 
        x:Name="MenuOpen" 
        Header="Open CSV File"
        Click="MenuOpen_Click"/>
                    <Separator/>
                    <MenuItem 
        x:Name="MenuClose"
        Header="Close"/>
                </MenuItem>

                <!--<MenuItem Header="Settings">
                <MenuItem x:Name="rSettings" 
                          Click="rSettings_Click"
                          Header="Rotor Settings"/>
            </MenuItem>-->
            </Menu>

        </StackPanel>

        <StatusBar Background="#F1EFEF"
VerticalAlignment="Bottom">
            <StatusBar VerticalAlignment="Bottom" Background="#F1EFEF" >
                <StackPanel Orientation="Horizontal">
                    <TextBlock x:Name="FilePathTextBlock" Text="File Path: " Margin="0,0,20,0"/>
                    <TextBlock x:Name="ringC" Text="Ring Count " Margin="0,0,20,0"/>
                    <TextBlock x:Name="rCount" Text="Character Count per Ring: " />
                </StackPanel>
            </StatusBar>
        </StatusBar>
